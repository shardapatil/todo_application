<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>UpdateTodoServlet</servlet-name>
  	<display-name>UpdateTodoServlet</display-name>
  	<description></description>
  	<servlet-class>com.todoapp.demo.UpdateTodoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UpdateTodoServlet</servlet-name>
  	<url-pattern>/updatetodo</url-pattern>
  </servlet-mapping>
  
  
  <welcome-file-list>
  	<welcome-file>todoadd.html</welcome-file>
  </welcome-file-list>
</web-app>
